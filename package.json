{
  "name": "@molgenis/molgenis-api-client",
  "version": "2.2.0",
  "description": "A javascript client for the molgenis api",
  "main": "dist/molgenis-api-client.es.js",
  "scripts": {
    "lint": "eslint src test tools",
    "test": "babel-node ./node_modules/istanbul/lib/cli.js cover ./node_modules/mocha/bin/_mocha",
    "debug": "mocha --compilers js:babel-register --debug-brk",
    "test:watch": "mocha --compilers js:babel-register --reporter min --watch",
    "build": "node build/build",
    "preversion": "yarn install; yarn test; yarn build"
  },
  "publishConfig": {
    "access": "public",
    "registry": "http://registry.npmjs.org"
  },
  "repository": "git@github.com:molgenis/molgenis-api-client.git",
  "keywords": [
    "molgenis",
    "api",
    "rest",
    "fetch"
  ],
  "author": "MOLGENIS Team, System Genetics, UMCG, Netherlands",
  "license": "LGPL-3.0",
  "dependencies": {
    "babel-runtime": "^6.11.6",
    "form-data": "^2.3.1",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.17.4"
  },
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-core": "^6.17.0",
    "babel-eslint": "^7.0.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-stage-2": "^6.16.0",
    "babel-register": "^6.16.3",
    "chai": "^4.0.0-canary.1",
    "coveralls": "^2.11.14",
    "del": "^2.2.2",
    "eslint": "^3.8.0",
    "eslint-config-airbnb-base": "^10.0.1",
    "eslint-plugin-import": "^2.2.0",
    "fetch-mock": "^5.12.1",
    "istanbul": "^1.1.0-alpha.1",
    "mocha": "^3.1.2",
    "rollup": "^0.36.3",
    "rollup-plugin-babel": "^2.6.1",
    "sinon": "^2.0.0-pre.3"
  },
  "babel": {
    "presets": [
      "latest",
      "stage-2"
    ],
    "plugins": [
      "transform-runtime"
    ]
  }
}
